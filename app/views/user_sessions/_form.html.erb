 <%= oauth_register_button :value => "Register with Twitter" %>
<%= oauth_login_button :value => "Login with Twitter" %>
 <div class="span3">

  <%= form_for @user_session, html:{class: 'form-signin'} do |f| %>
   <h2 class="form-signin-heading">Please sign in</h2>

  <% if @user_session.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@user_session.errors.count, "error") %> prohibited this user_session from being saved:</h2>

      <ul>
      <% @user_session.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :username %><br />
    <%= f.text_field :username,:class=>"input-block-level" %>
  </div>
  <div class="field">
    <%= f.label :password %><br />
    <%= f.text_field :password ,:class=>"input-block-level"%>
  </div>
  <div class="actions">
    <%= f.submit, :class=>"btn btn-large btn-primary" %>
  </div>
<% end %>


      

